<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="inherit_survey_page_print" inherit_id="survey.survey_page_print">
        <xpath expr="//t[@t-foreach='survey.question_and_page_ids']" position="before">
            <t t-set="min_sequence" t-value="False" />
            <t t-set="min_question" t-value="None" />

            <t t-foreach="survey.question_and_page_ids" t-as="question">
                <t t-if="question.is_normative_filter">
                    <t
            t-if="min_sequence == False or question.sequence &lt; min_sequence"
          >
                        <t t-set="min_sequence" t-value="question.sequence" />
                        <t t-set="min_question" t-value="question" />
                    </t>
                </t>
            </t>

            <t
        t-set="filtered_questions"
        t-value="survey.question_and_page_ids.filter(lambda q: q.is_normative_filter and q != min_question)"
      />

            <t t-set="survey.question_and_page_ids" t-value="filtered_questions" />
        </xpath>
    </template>
</odoo>
